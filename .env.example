# ReTodoku Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Cloudflare D1 Database Configuration
# 数据库名称（已配置）
VITE_DB_NAME=retodoku

# 数据库ID（您的实际数据库ID）
VITE_DB_ID=your-database-id-here

# Cloudflare Account ID（从wrangler whoami获取）
VITE_CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id-here

# API Base URL（如果部署Workers API）
VITE_API_BASE_URL=https://your-worker-name.your-subdomain.workers.dev/api

# 开发环境配置
NODE_ENV=development


# Development environment
VITE_DB_NAME=retodoku-dev-db
VITE_DB_ID=your-dev-database-id-here

# Production environment (for deployment)
VITE_PROD_DB_NAME=retodoku-prod-db
VITE_PROD_DB_ID=your-prod-database-id-here 